{
    "presets": [
        "next/babel"
    ],
    "plugins": [
        [
            "babel-plugin-styled-components",
            {
                "ssr": true,
                "displayName": true,
                "preprocess": false
            }
        ],
        [
            "babel-plugin-root-import",
            {
                "rootPathSuffix": "./"
            }
        ]
    ]
}